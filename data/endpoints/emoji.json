{
    "name": "EmojiEndpoints",
    "requires": [
        "Optional",
        "Snowflake",
        "Unset"
    ],
    "methods": {
        "list_guild_emojis": {
            "method": "GET",
            "url": "/guilds/{guild_id}/emojis",
            "url-parameters": {
                "guild_id": "Snowflake"
            }
        },
        "get_guild_emoji": {
            "method": "GET",
            "url": "/guilds/{guild_id}/emojis/{emoji_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "emoji_id": "Snowflake"
            }
        },
        "create_guild_emoji": {
            "method": "POST",
            "url": "/guilds/{guild_id}/emojis",
            "url-parameters": {
                "guild_id": "Snowflake"
            },
            "json-parameters": {
                "name": "str",
                "image": "str",
                "roles": "list[Snowflake]"
            },
            "supports-reason": true
        },
        "modify_guild_emoji": {
            "method": "PATCH",
            "url": "/guilds/{guild_id}/emojis/{emoji_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "emoji_id": "Snowflake"
            },
            "json-parameters": {
                "name": [
                    "UnsetOr[str]",
                    "Unset"
                ],
                "roles": [
                    "UnsetOr[list[Snowflake]]",
                    "Unset"
                ]
            },
            "supports-reason": true
        },
        "delete_guild_emoji": {
            "method": "DELETE",
            "url": "/guilds/{guild_id}/emojis/{emoji_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "emoji_id": "Snowflake"
            },
            "supports-reason": true
        }
    }
}
