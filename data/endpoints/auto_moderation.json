{
    "name": "AutoModerationEndpoints",
    "methods": {
        "list_auto_moderation_rules": {
            "method": "GET",
            "url": "/guilds/{guild_id}/auto-moderation/rules",
            "url-parameters": {
                "guild_id": "Snowflake"
            }
        },
        "get_auto_moderation_rule": {
            "method": "GET",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            }
        },
        "create_auto_moderation_rule": {
            "method": "POST",
            "url": "/guilds/{guild_id}/auto-moderation/rules",
            "url-parameters": {
                "guild_id": "Snowflake"
            },
            "json-parameters": {
                "name": "str",
                "event_type": "int",
                "trigger_type": "int",
                "trigger_metadata": ["Union[dict[str, Any], ellipsis]", "..."],
                "actions": "list[discord_typings.AutoModerationActionData]",
                "enabled": ["Union[bool, ellipsis]", "..."],
                "exempt_roles": ["Union[list[Snowflake], ellipsis]", "..."],
                "exempt_channels": ["Union[list[Snowflake], ellipsis]", "..."]
            },
            "supports-reason": true
        },
        "modify_auto_moderation_rule": {
            "method": "PATCH",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            },
            "json-parameters": {
                "name": ["Union[str, ellipsis]", "..."],
                "event_type": ["Union[int, ellipsis]", "..."],
                "trigger_type": ["Union[int, ellipsis]", "..."],
                "trigger_metadata": ["Union[dict[str, Any], ellipsis]", "..."],
                "actions": ["Union[list[discord_typings.AutoModerationActionData], ellipsis]", "..."],
                "enabled": ["Union[bool, ellipsis]", "..."],
                "exempt_roles": ["Union[list[Snowflake], ellipsis]", "..."],
                "exempt_channels": ["Union[list[Snowflake], ellipsis]", "..."]
            },
            "supports-reason": true
        },
        "delete_auto_moderation_rule": {
            "method": "DELETE",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            },
            "supports-reason": true
        }
    }
}
