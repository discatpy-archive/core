{
    "name": "AutoModerationEndpoints",
    "requires": [
        "Any",
        "Optional",
        "discord_typings",
        "Snowflake",
        "Unset"
    ],
    "methods": {
        "list_auto_moderation_rules": {
            "method": "GET",
            "url": "/guilds/{guild_id}/auto-moderation/rules",
            "url-parameters": {
                "guild_id": "Snowflake"
            }
        },
        "get_auto_moderation_rule": {
            "method": "GET",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            }
        },
        "create_auto_moderation_rule": {
            "method": "POST",
            "url": "/guilds/{guild_id}/auto-moderation/rules",
            "url-parameters": {
                "guild_id": "Snowflake"
            },
            "json-parameters": {
                "name": "str",
                "event_type": "discord_typings.AutoModerationEventTypes",
                "trigger_type": "discord_typings.AutoModerationTriggerTypes",
                "trigger_metadata": [
                    "discord_typings.AutoModerationTriggerMetadataData",
                    "Unset"
                ],
                "actions": "list[discord_typings.AutoModerationActionData]",
                "enabled": [
                    "bool",
                    "Unset"
                ],
                "exempt_roles": [
                    "list[Snowflake]",
                    "Unset"
                ],
                "exempt_channels": [
                    "list[Snowflake]",
                    "Unset"
                ]
            },
            "supports-reason": true
        },
        "modify_auto_moderation_rule": {
            "method": "PATCH",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            },
            "json-parameters": {
                "name": [
                    "str",
                    "Unset"
                ],
                "event_type": [
                    "discord_typings.AutoModerationEventTypes",
                    "Unset"
                ],
                "trigger_metadata": [
                    "discord_typings.AutoModerationTriggerMetadataData",
                    "Unset"
                ],
                "actions": [
                    "list[discord_typings.AutoModerationActionData]",
                    "Unset"
                ],
                "enabled": [
                    "bool",
                    "Unset"
                ],
                "exempt_roles": [
                    "list[Snowflake]",
                    "Unset"
                ],
                "exempt_channels": [
                    "list[Snowflake]",
                    "Unset"
                ]
            },
            "supports-reason": true
        },
        "delete_auto_moderation_rule": {
            "method": "DELETE",
            "url": "/guilds/{guild_id}/auto-moderation/rules/{auto_moderation_rule_id}",
            "url-parameters": {
                "guild_id": "Snowflake",
                "auto_moderation_rule_id": "Snowflake"
            },
            "supports-reason": true
        }
    }
}
