{
    "name": "InviteEndpoints",
    "methods": {
        "get_invite": {
            "method": "GET",
            "url": "/invites/{invite_code}",
            "url-parameters": {
                "invite_code": "Snowflake"
            },
            "query-parameters": {
                "with_counts": [
                    "bool",
                    "Unset"
                ],
                "with_expiration": [
                    "bool",
                    "Unset"
                ],
                "guild_scheduled_event_id": [
                    "Snowflake",
                    "Unset"
                ]
            }
        },
        "delete_invite": {
            "method": "DELETE",
            "url": "/invites/{invite_code}",
            "url-parameters": {
                "invite_code": "Snowflake"
            },
            "supports-reason": true
        }
    }
}
